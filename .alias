#pake `type namaalias` untuk ngecek apakah namaalias sudah terpakai
alias lc="colorls"
alias hackmode="cd ~/cool-retro-term && ./cool-retro-term"

# restart ke windows
alias restart="sudo grub-reboot 2 && sudo reboot"
alias run="javac Main.java && java Main"
# alias updatevsc="/home/imampt/Documents/Clone\ Repo/vscode-updater/update.sh"
alias updatevsc="/home/imampt/updatevsc.sh"
alias sql="/home/imampt/bin/loginmysql"
alias piano="cd /home/imampt/bin/clones/piano-rs;./target/release/piano-rs"
alias tohtdocs="cd /opt/lampp/htdocs"
alias xsql="/opt/lampp/bin/mysql -u root -p"
alias edit=vim
alias op="xdg-open"
alias sudo='sudo '
alias man='man '
alias hs='history | grep'
alias gdcol='git diff --color-words=.'
alias gdchar='git diff --word-diff-regex=.'
alias hst='history'
alias sglog='git log --oneline --graph --all --author-date-order'
alias dglog='git log --graph --pretty=format:"%C(bold blue)%s%C(auto)%d%n%C(yellow)%h%Creset %an, %C(magenta)%ar%Creset at %C(cyan)%ad%n%b" --date=format:"%H:%M"' 
alias gglog='git log --graph --pretty=format:"%C(bold blue)%s%C(auto)%d%n%C(yellow)%h%Creset %an, %C(magenta)%ar%Creset at %C(cyan)%ad%n%b" --date=format:"%H:%M" --author-date-order'
alias ggloga='gglog --all'
alias gglogs='gglog --stat'
alias gglogd='gglog -p -m --histogram'
alias enin='~/bin/script/GT-bash-client/translate.sh en in'
alias inen='~/bin/script/GT-bash-client/translate.sh in en'
alias ud='~/bin/script/ud/ud'
alias getc='history | tail -n 1 | sed "s/[[:digit:]]*  //" | sed "s/^#//" | tr --delete '\n' | xclip -selection clipboard'
alias wpart='lsblk | grep sda | grep G | grep -Ev "disk|/|SWAP"'
alias view='google-chrome'
